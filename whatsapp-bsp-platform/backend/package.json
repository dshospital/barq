    "sharp": "^0.33.1",
    "puppeteer": "^21.6.1",
    "handlebars": "^4.7.8",
    "nodemailer": "^6.9.7",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "prom-client": "^15.1.0",
    "@willsoto/nestjs-prometheus": "^6.0.0",
