{
  "name": "whatsapp-bsp-platform",
  "version": "1.0.0",
  "description": "WhatsApp Business Solution Provider Platform - High-Scalability SaaS",
  "author": "BSP Platform Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/api-gateway/main",
    "start:webhook": "nest start webhook-handler",
    "start:worker": "nest start message-worker",
    "start:campaign": "nest start campaign-service",
    "start:template": "nest start template-service",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api-gateway/test/jest-e2e.json",
    "migration:generate": "typeorm migration:generate",
    "migration:run": "typeorm migration:run",
    "migration:revert": "typeorm migration:revert",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "seed": "ts-node scripts/seed.ts"
  },
